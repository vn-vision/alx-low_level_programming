#include <stdio.h>

/**
* main -Entry point
* Return: 0 (success)
* print_last_digit - prints the last digit of a number
*
*/

int print_last_digit(int);

int main(void)
{
	int r;

	print_last_digit(98);
	print_last_digit(0);
	r = print_last_digit(-1024);

	putchar('0' + r);
	puthar('\n');

	return (0);
}

int print_last_digit(int n)
{
	int lsdgt;

	lsdgt = n%10;

	return (lsdgt);
}
